@charset "UTF-8";
/* reset */body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,fieldset,legend,textarea,p,blockquote,th,td,input,select,textarea,button{margin:0;padding:0}
fieldset,img{border:0 none}
dl,ul,ol,menu,li{list-style:none}
blockquote,q{quotes:'none'}
blockquote:before,blockquote:after,q:before,q:after{content:'';content:'none'}
input,select,textarea,button{vertical-align:middle}
input::-ms-clear{display:none}
button{border:0 none;background-color:transparent;cursor:pointer}
body{background:#fff}
body,th,td,input,select,textarea,button{font-size:12px;line-height:1.5;font-family:'돋움',dotum,sans-serif;color:#333}
a{color:#333;text-decoration:none}
a:active,a:hover{text-decoration:underline}
a:active{background-color:transparent}
address,caption,cite,code,dfn,em,var{font-style:normal;font-weight:normal}

/* global */
#kakaoIndex{overflow:hidden;position:absolute;left:-9999px;width:0;height:1px;margin:0;padding:0}
.ir_wa{display:block;overflow:hidden;position:relative;z-index:-1;width:100%;height:100%}
.ir_pm{display:block;overflow:hidden;font-size:0;line-height:0;text-indent:-9999px}
.screen_out{overflow:hidden;position:absolute;width:0;height:0;line-height:0;text-indent:-9999px}

/* Background Image */
.ico_comm{display:block;overflow:hidden;background:url(../images/m320/ico_comm.png) no-repeat 0 0;text-indent:-9999px}



/* wrap_search */
#kakaoWrap {min-width:320px}
.cont_main{width:100%;margin:0 auto;background:#eff2f5}


.head_info {position:relative;z-index:0;width:100%;height:44px;border-top:1px solid #434753;border-bottom:1px solid #2e313b;background-color:#3d414d;border-bottom:6px solid #5182f5}
.head_info .tit_daum {float:left;padding-left:8px}
.head_info .link_daum {float:left;height:31px;padding-top:11px;font-size:0;line-height:0;vertical-align:top}
.head_info .link_service {float:left;height:18px;margin-left:6px;padding-top:12px;vertical-align:top}
.head_info .tit_service {font-size:16px;color:#fff;vertical-align:top}
.head_info .link_relate {float:left;height:42px;margin-left:10px;font-weight:bold;font-size:15px;line-height:43px;letter-spacing:-1px;color:#8d95a2;-webkit-text-shadow:0 1px 0 #2b2d36;text-shadow:0 1px 0 #2b2d36}
.head_info .wrap_util {float:right;padding-right:4px}
.head_info .link_util {display:inline-block;float:left;width:35px;height:31px;margin-right:1px;padding-top:12px}
.head_info .ico_search {display:block;width:19px;height:19px;margin:0 auto;background-position:-87px -22px}
.head_info .ico_all {width:18px;height:18px;margin:0 auto;background-position:0 -36px}
.head_info .link_app {width:36px;height:33px;margin:0 -2px 0 2px;padding-top:9px;text-align:center}

.main_wrap .info_head{overflow:hidden;padding:12px 10px;border-bottom:1px solid #d2d8de}
.tf_date {padding:10px 10px;border-top:1px solid #fff}
.info_head .mycash_info{position:relative;float:left}
.info_head .my_cash{padding-right:10px;font-size:14px;font-weight:bold}
.info_head .btn_mypage{float:right;border:1px solid #aab3bc;border-radius:5px;padding:5px 4px;background:url(../images/m320/btn_bg.png) repeat-x;color:#646b78;box-shadow:1px 1px 1px #e1e4e7}
.info_head .my_money{color:#168df3;font-size:19px;font-weight:bold;width:100px;position:absolute;top:0;left:85px}
.info_head .my_cash:before{display:inline-block;width:22px;height:23px;content:'';background:url(../images/m320/ico_comm.png) no-repeat;background-position:0 -60px;vertical-align:middle;padding-left:6px;margin-top:-4px}


.select_date{background:#fff;border:1px solid #c5cdd5;margin-bottom:10px}
.opt_league{position:relative;float:left;height:29px}
.opt_league .link_selected{position:relative;display:block;width:100px;height:28px;text-indent:5px;font-weight:bold;letter-spacing:-1px;font-size:15px;box-sizing:border-box;line-height:29px;border:1px solid #949da6;text-align:left}
.opt_league .link_selected:before{content:'';width:27px;height:28px;position:absolute;top:-1px;right:-1px;background:url(../images/m320/ico_comm.png) no-repeat 0 0;}
.opt_league .link_card{width:160px}
.box_amount{overflow:hidden;border-bottom:1px solid #dce1e6;;padding:10px 12px}
.box_amount .txt_info{float:left;font-size:14px}
.box_amount .desc_txt{float:right;color:#6f737b}
.box_amount .desc_txt:before{display:inline-block;width:13px;height:14px;content:'';background:url(../images/m320/ico_comm.png) no-repeat;background-position:0 -84px;vertical-align:middle;padding-left:3px}
.box_date{padding:11px 12px;overflow:hidden}
.date .info_choose{overflow:hidden;position:relative;margin-bottom:8px;width:215px;height:32px}
.opt_choose{position:absolute;width:100%;height:30px;top:0px;left:0px;opacity:0;border:0 none}
.info_choose{overflow:hidden;position:relative;width:100%;margin-bottom:10px}
.info_choose .inp_direct{width:100%;height:30px;background-position:0 -84px}
.info_choose .input_wrap{float:left;width:100px;height:30px;background-position:0 0}
.info_choose .inp_st{display:block;float:left;width:100px;height:30px;background-color:transparent;padding-left:6px;border:1px solid #949da6;box-sizing:border-box}
.info_choose .inp_choose{float:left;position:absolute;top:0;left:0;width:16px;height:18px;opacity:0.01;border:none;cursor:pointer}
.info_choose .lab_check{float:left;height:19px;background-position:0 -108px;padding:0 10px 0 20px;line-height:20px;text-indent:0;cursor:pointer;color:#666;margin-top:6px}
.info_choose .inp_choose:checked + .lab_check{background-position:0 -132px}
.select_payment .info_choose .inp_choose:checked + .lab_check{font-weight:bold}
.txt_won{line-height:32px;padding-left:5px;color:#666;float:left}

.select_payment{overflow:hidden}
.select_payment .info_choose{float:left;width:50%;text-align:center}

.info_check{position:relative}
.info_check .inp_check{position:absolute;top:0;left:0;opacity:0.01;width:16px;height:17px;float:left;cursor:pointer}
.info_check .lab_check{background-position:0 -156px;text-indent:0;padding-left:20px;cursor:pointer}
.info_check .inp_check:checked + .lab_check{background-position:0 -180px;font-weight:bold}
.box_ch{float:left}
.wrap_btn{display:table;table-layout:fixed;width:100%;text-align:center;padding-bottom:20px;box-sizing:border-box;padding:10px 10px}
.inner_btn{display:table-cell}
.inner_btn .link_refuse{display:block;height:48px;box-sizing:border-box;padding-right:4px}
.inner_btn .link_refuse{display:block;height:48px;box-sizing:border-box;padding-left:4px}
.inner_btn .btn_cancel{display:block;border:1px solid #afb7c0;box-sizing:border-box;background:#fbfbfb;line-height:32px;font-size:14px}
.inner_btn .btn_next{display:block;border:1px solid #3579d9;box-sizing:border-box;background:#4597e4;line-height:32px;font-size:14px;color:#fff}

/* foot */
.foot_wrap {width:100%;margin:0 auto;padding-top:10px;border-top:1px solid #d9d9d9;font-family:sans-serif;background-color:#f1f1f1;text-align:center;clear:both}
.foot_wrap .list_foot {display:inline-block;width:auto;height:28px;margin:0 auto;text-align:center;*zoom:1}
.foot_wrap .list_foot li {display:inline}
.foot_wrap .btn_foot {display:inline-block;width:64px;height:25px;border:1px solid #c4c4c4;border-radius:3px;font-size:13px;line-height:26px;background-color:#fbfbfb;background-image:-webkit-gradient(linear, left top, left bottom, from(#fff), to(#f6f6f6));background-image:-moz-linear-gradient(top, #fff, #f6f6f6);background-image:-o-linear-gradient(top, #fff, #f6f6f6);background-image:linear-gradient(to bottom, #fff, #f6f6f6);color:#444;letter-spacing:-1px;vertical-align:top}
.foot_wrap .btn_off {color:#8b8b8b;cursor:default}
.foot_wrap .copr_info {display:block;padding:4px 0 6px;font-size:11px;color:#888;clear:both}
.foot_wrap .info_policy {display:block;padding:6px 0 8px;font-weight:bold;font-size:10px;line-height:10px}
.foot_wrap .link_policy {margin:0 5px;color:#888;text-decoration:underline}
.foot_wrap .link_daumcorp {color:#888}
